{
    "name": "codexshaper/laravel-woocommerce",
    "description": "WooCommerce Rest API with Laravel",
    "type": "library",
    "require": {
        "automattic/woocommerce": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Md Abu Ahsan Basir",
            "email": "maab.career@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Codexshaper\\WooCommerce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codexshaper\\WooCommerce\\WooCommerceServiceProvider"
            ],
            "aliases": {
                "WooCommerce": "Codexshaper\\WooCommerce\\Facades\\WooCommerce",
                "Product": "Codexshaper\\WooCommerce\\Facades\\Product",
                "Order": "Codexshaper\\WooCommerce\\Facades\\Order",
                "Customer": "Codexshaper\\WooCommerce\\Facades\\Customer"
            }
        }
    }
}